{
  "name": "Discord Toggle Panel",
  "manifest_version": 2,
  "version": "0.6",
  "description": "Toggle channels and servers tab in discord.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.discordapp.com/channels/*","https://*.discordapp.com/activity/*"],
      "run_at": "document_end",
      "all_frames": true,
      "js": ["myFunction.js"]
    }
  ],
    "icons": {
      "16": "icon32.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon48.png"
    }
}
  

  